set -g mode-style "fg=${thm_fg},bg=${thm_bg}"
set -g message-style "fg=${thm_fg},bg=${thm_bg}"
set -g message-command-style "fg=${thm_fg},bg=${thm_bg}"
set -g pane-border-style "fg=${thm_inactive_pane_bg}"
set -g pane-active-border-style "fg=${thm_active_pane_bg}"
set -g status "on"
set -g status-justify "left"
set -g status-style "fg=${thm_fg},bg=${thm_bg}"
set -g status-left-length "100"
set -g status-right-length "100"
set -g status-left-style NONE
set -g status-right-style NONE
set -g status-left "#[fg=${thm_active_pane_fg},bg=${thm_active_pane_bg}, bold]#{?client_prefix, #[fg=${thm_command_mode_bg}]󰣇 ,#[fg=${thm_active_pane_fg}] 󰣇 }#[fg=${thm_active_pane_bg},bg=${thm_inactive_pane_bg},nobold,nounderscore,noitalics]#[fg=${thm_inactive_pane_fg},bg=${thm_inactive_pane_bg}, bold] #S #[fg=${thm_inactive_pane_bg},bg=${thm_bg}]"
set -g status-right "#[fg=${thm_fg},bg=${thm_bg}] #{prefix_highlight} #[fg=${thm_inactive_pane_bg},bg=${thm_bg}]#[fg=${thm_inactive_pane_fg},bg=${thm_inactive_pane_bg}] #(date -I) #[fg=${thm_active_pane_bg},bg=${thm_inactive_pane_bg},nobold,nounderscore,noitalics]#[fg=${thm_active_pane_fg},bg=${thm_active_pane_bg}, bold]  %H:%M "
setw -g window-status-activity-style "underscore,fg=${thm_fg},bg=${thm_bg}"
setw -g window-status-separator ""
setw -g window-status-style "NONE,fg=${thm_inactive_pane_fg},bg=${thm_inactive_pane_bg}"
setw -g window-status-format "#[fg=${thm_bg},bg=${thm_inactive_pane_bg},nobold,nounderscore,noitalics]#[default] #I  #W #F #[fg=${thm_inactive_pane_bg},bg=${thm_bg},nobold,nounderscore,noitalics]"
setw -g window-status-current-format "#[fg=${thm_bg},bg=${thm_active_pane_bg},nobold,nounderscore,noitalics]#[fg=${thm_active_pane_fg},bg=${thm_active_pane_bg},nobold] #I  #W #[fg=${thm_active_pane_bg},bg=${thm_bg},nobold,nounderscore,noitalics]"
