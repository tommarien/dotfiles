set -g mode-style "fg=${thm_selection_fg},bg=${thm_selection_bg}"
set -g message-style "fg=${thm_fg},bg=default"
set -g message-command-style "fg=${thm_fg},bg=default"
set -g pane-border-style "fg=${thm_inactive_pane_bg}"
set -g pane-active-border-style "fg=${thm_active_pane_bg}"
set -g status "on"
set -g status-justify "absolute-centre"
set -g status-style "fg=${thm_fg},bg=default"
set -g status-left-length "100"
set -g status-right-length "100"
set -g status-left-style NONE
set -g status-right-style NONE
set -g status-left "#[fg=${thm_active_pane_fg},bg=${thm_active_pane_bg}, bold]#{?client_prefix, #[fg=${thm_command_mode_bg}]󰊠 ,#[fg=${thm_active_pane_fg}] 󰊠 }#[fg=${thm_active_pane_fg},bg=${thm_active_pane_bg}, bold] #S #[fg=${thm_active_pane_bg},bg=default,nobold,nounderscore,noitalics]"
set -g status-right "#[fg=${thm_selection_fg},bg=default]  #(date -I) #[fg=${thm_active_pane_bg},bg=default,nobold,nounderscore,noitalics]#[fg=${thm_active_pane_fg},bg=${thm_active_pane_bg}, bold]  %H:%M "
setw -g window-status-activity-style "underscore,fg=${thm_fg},bg=${thm_bg}"
setw -g window-status-separator ""
setw -g window-status-style "NONE,fg=${thm_fg},bg=default"
setw -g window-status-format "#[fg=${thm_selection_bg},bg=default] #I-#W "
setw -g window-status-current-format "#[fg=${thm_active_pane_bg},bg=${thm_bg},nobold] 󰮯  #I-#W "
